<variables>
	<variable name="psh" place="in" type="quality" state="disabled" description="Коэффициент шероховатости"/>
	<variable name="vr" place="in" type="quality" state="enabled" description="Скорость реки"/>
	<variable name="qst" place="in" type="quality" state="enabled" description="Расход сточных вод на оголовок"/>
	<variable name="cct" place="in" type="quality" state="enabled" description="Величина загрязняющего вещества"/>
	<variable name="pc" place="out" type="quality" state="disabled" description="Коэффициент Шези">
		<parameter name="psh"/>
		<parameter name="h"/>
	</variable>
	<variable name="dog" place="in_out" type="quality" state="enabled" description="Диаметр оголовка">
		<parameter name="qst"/>
	</variable>
	<variable name="vst" place="out" type="quality" state="enabled" description="Скорость сточных вод (на оголовок)">
		<parameter name="qst"/>
		<parameter name="dog"/>
	</variable>
	<variable name="dt" place="out" type="quality" state="enabled" description="Относительный диаметр"/>
	<variable name="dzz" place="out" type="quality" state="enabled" description="Диаметр пятна загрязнения">
		<parameter>vr</parameter>
	</variable>
	<variable name="nn" place="in_out" type="quality" state="enabled" description="Начальное разбавление">
		<parameter>vst</parameter>
	</variable>
	<variable name="pd" place="in_out" type="quality" state="enabled" description="Коэффициент диффузии">
		<parameter name="vr"/>
		<parameter name="h"/>
		<parameter name="pc"/>
	</variable>
	<variable name="xn" place="in_out" type="quality" state="enabled" options="shift" description="Расстояние от створа выпуска">
		<parameter name="vr"/>
		<parameter name="vst"/>
		<parameter name="dzz"/>
	</variable>
	<variable name="dz" place="out" type="quality" state="enabled" options="delta" description="dz">
		<parameter name="n"/>
		<parameter name="dzz"/>
	</variable>
	<variable name="dy" place="out" type="quality" state="enabled" options="delta" description="dy">
		<parameter name="dz"/>
	</variable>
	<variable name="dx" place="out" type="quality" state="enabled" options="delta" description="dx">
		<parameter name="vr"/>
		<parameter name="pd"/>
		<parameter name="dy"/>
	</variable>
	<variable name="nog" place="in" type="quantity" state="enabled" options="max" description="Количество оголовков"/>
	<variable name="nl" place="in" type="quantity" state="enabled" options="max" description="Количество промежуточных сечений"/>
	<variable name="n" place="in" type="quantity" state="enabled" description="Сторона расчетного квадрата (в ячейках)"/>
	<variable name="br" place="in" type="geometry" state="enabled" description="Расстояние до правого берега">
		<child_variable name="rbr" place="out" type="geometry" state="enabled" delta="dy" description="Расстояние до правого берега (расч.)"/>
	</variable>
	<variable name="b" place="in" type="geometry" state="enabled" description="Расстояния между оголовками">
		<child_variable name="rb" place="out" type="geometry" state="enabled" max="nog" delta="dy" description="Расстояния между оголовками (расч.)"/>
	</variable>
	<variable name="bl" place="in" type="geometry" state="enabled" description="Расстояние до левого берега">
		<child_variable name="rbl" place="out" type="geometry" state="enabled" delta="dy" description="Расстояние до левого берега (расч.)"/>
	</variable>
	<variable name="w" place="in" type="geometry" state="enabled" description="Общая ширина">
		<parameter name="br"/>
		<parameter name="b"/>
		<parameter name="bl"/>
		<child_variable name="rw" place="out" type="geometry" state="enabled" delta="dy" description="Общая ширина (расч.)">
			<parameter name="rbr"/>
			<parameter name="rb"/>
			<parameter name="rbl"/>
		</child_variable> 
	</variable>
	<variable name="hog" place="in" type="geometry" state="enabled" max="nog" description="Высоты оголовков">
		<child_variable name="rhog" place="out" type="geometry" state="enabled" delta="dy" description="Высоты оголовков (расч.)"/>
	</variable>
	<variable name="h" place="in" type="geometry" state="enabled" description="Глубина">
		<child_variable name="rh" place="out" type="geometry" state="enabled" delta="dy" description="Глубина (расч.)"/>
	</variable>
	<variable name="l" place="in" type="geometry" state="enabled" description="Промежуточные сечения (доп.)">
		<child_variable name="rl" place="out" type="geometry" state="enabled" max="nl" delta="dx" shift="xn" description="Промежуточные сечения (расч.)"/>
	</variable>
	<variable name="ll" place="in" type="geometry" state="enabled" description="Протяженность расчетного участка реки">
		<child_variable name="rll" place="out" type="geometry" state="enabled" delta="dx" shift="xn" description="Протяженность расчетного участка реки (расч.)"/>
	</variable>
	<variable name="cut" place="final" type="section" state="enabled" description="Сечение">
		<parameter name="n"/>
		<parameter name="nn"/>
		<parameter name="cct"/>
		<parameter name="rbr"/>
		<parameter name="rb"/>
		<parameter name="rbl"/>
		<parameter name="rw"/>
		<parameter name="rh"/>
		<parameter name="rhog"/>
	</variable>
</variables>
